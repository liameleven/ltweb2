<div class="card">
    <div class="card-header border-transparent">
        <h3 class="card-title">Editor</h3>
    </div>
    <div class="card-body p-0">
        <div class="table-responsive">
            <table class="table m-0">
                <thead>
                    <tr role="row">
                        <th class="sorting_asc" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending">ID
                        </th>
                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-label="Browser: activate to sort column ascending">User Name
                        </th>
                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-label="Browser: activate to sort column ascending">Email
                        </th>
                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-label="Browser: activate to sort column ascending">Birthday
                        </th>
                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-label="Browser: activate to sort column ascending">Gender
                        </th>
                        <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1"
                            aria-label="Browser: activate to sort column ascending">Phone
                        </th>
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr role="row" class="odd">
                        <td tabindex="0" class="sorting_1">{{uid}}</td>
                        <td>{{user_name}}</td>
                        <td>{{email}}</td>
                        <td>{{birthday}}</td>
                        <td>{{gender}}</td>
                        <td>{{phone}}
                            <a class="btn btn-primary btn-sm" href="/dashboard/admin/editor/manager-editor?uid={{uid}}"
                                role="button" title="Manager Editor" style="float: right;">
                                <i class="fas fa-edit"></i>
                            </a>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    <div class="card-footer clearfix">
        <nav aria-label="..." style="float: left;">
            <ul class="pagination d-flex justify-content-center">
                <li class="page-item {{#unless can_go_prev}}disabled{{/unless}}">
                    <a class="page-link" href="/dashboard/admin/subscriber?page={{prev_value}}" tabindex="-1"
                        aria-disabled="true">Previous</a>
                </li>
                {{#each page_items}}
                <li class="page-item {{#if isActive}}active{{/if}}"><a class="page-link"
                        href="/dashboard/subscriber?page={{value}}">{{value}}</a>
                </li>
                {{/each}}
                <li class="page-item {{#unless can_go_next}}disabled{{/unless}}">
                    <a class="page-link" href="/dashboard/admin/subscriber?page={{next_value}}">Next</a>
                </li>
            </ul>
        </nav>
    </div>
</div>