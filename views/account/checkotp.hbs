<body>
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
				<form class="login100-form validate-form" method="POST" action="">
					<span class="login100-form-title p-b-33">
						CHECK OTP   
					</span>

					<div class="wrap-input100 validate-input" data-validate = "Valid OTP is required: 123456">
						<input class="input100" type="text" name="otp" placeholder="OTP">
						<span class="focus-input100-1"></span>
						<span class="focus-input100-2"></span>
					</div>
					{{#if err}}
                        <p style="color: red;">{{err}}</p>
                    {{/if}}
					<div class="p-t-15">
                            <button class="btn btn--radius-2 btn--blue" id="btnSubmit" type="submit">Check</button>
                    </div>
				</form>
			</div>
		</div>
	</div>
	

	
